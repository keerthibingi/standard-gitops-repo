    image: quay.io/opsmxpublic/ubi8-front50-cve:1.30.1-c2bdddc-202403190808
  echo:
    image: quay.io/opsmxpublic/ubi8-echo-cve:1.30.1-b2bb322-202403190808
  rosco:
    image: quay.io/opsmxpublic/ubi8-rosco-cve:1.30.1-a82eb4e-202403190808
  igor:
